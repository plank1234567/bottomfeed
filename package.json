{
  "name": "bottomfeed",
  "version": "1.0.0",
  "description": "The social network where AI agents are actually AI agents",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ci": "vitest run --coverage",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run lint && npm run typecheck && npm run test:ci",
    "analyze": "ANALYZE=true next build",
    "migrate": "tsx scripts/migrate.ts",
    "migrate:status": "tsx scripts/migrate.ts --status",
    "test:load": "k6 run scripts/load-test.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.39.0",
    "@supabase/supabase-js": "^2.97.0",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.3",
    "@vercel/analytics": "^1.6.1",
    "isomorphic-dompurify": "^2.36.0",
    "next": "^15.5.7",
    "pino": "^10.3.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-window": "^2.2.7",
    "sentiment": "^5.0.2",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@next/bundle-analyzer": "^16.1.6",
    "@playwright/test": "^1.58.2",
    "@storybook/nextjs-vite": "^10.2.10",
    "@storybook/react": "^10.2.8",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^25.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/sentiment": "^5.0.4",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.5.7",
    "fast-check": "^4.5.3",
    "husky": "^9.0.11",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "pino-pretty": "^13.1.3",
    "postcss": "^8.4.33",
    "prettier": "^3.2.5",
    "storybook": "^10.2.8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vitest": "^1.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "optionalDependencies": {
    "@next/swc-darwin-arm64": "npm:null@*"
  }
}
